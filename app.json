{
  "name": "Naesan WhatsApp Bot",
  "description": "WhatsApp Bot running on Javascript",
  "keywords": [
    "whatsapp",
    "bot",
    "plugin",
    "modular",
    "productivity"
  ],
  "repository": "https://github.com/AnasBex/naesan-wa",
  "website": "",
  "stack": "container",
  "env": {
    "LOGGING": {
      "description": "ID group for logging",
      "required": false
    },
    "OWNER_PHONE": {
      "description": "Nomor AnasBex.",
      "required": true
    },
    "BOT_NUMBER": {
      "description": "Nomor Bot.",
      "required": true
    },
    "KEY_REMOVEBG": {
      "description": "Enter remove.bg api key.",
      "required": true
    },
    "API_KEY": {
      "description": "Masukkan api key universal mean seperti api rest tapi gunakan semua fiture.",
      "required": false
    },
    "WEB_API": {
      "description": "Masukkan api web awal untuk auth Anda.",
      "required": false
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "14"
      }
    }
  ]
}
